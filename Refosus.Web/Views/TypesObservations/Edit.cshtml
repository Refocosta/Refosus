@{
    ViewData["Title"] = "CRM | CANALES";
    Layout = "~/Views/Shared/_Refosus1.cshtml";
}
<div class="container col-md-11" >
    <br />
    <a asp-controller="TypesObservations" asp-action="Index" class="btn btn-outline-primary" role="button">Atras</a>
    <div class="card mt-2" >
        <div class="card-header" >
            <center><h3>Editar</h3></center>
        </div>
        <div class="card-body">
            <input type="hidden" id="editId" key="@ViewBag.Id" />
            <div class="row">
                <div class="col-md-4">
                    <label>Nombre</label>
                    <input id="nameEdit" class="form-control" />
                </div>
                <div class="col-md-4">
                    <label>Fecha de creación</label>
                    <input id="createdEdit" class="form-control" readonly />
                </div>
                <div class="col-md-4">
                    <label>Fechal de ultima actualización</label>
                    <input id="updatedEdit" class="form-control" readonly />
                </div>
            </div>
            <hr />
            <button id="editTypesObservations" class="btn btn-outline-success" >Editar</button>
        </div>
    </div>
</div>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <script src="~/js/Fetch.js"></script>
    <script src="~/js/TypesObservations.js"></script>
}
